structs:
	A1 = combine ( C , { H, V } );
	A2 = combine ( C , { H, V } );

domain-hidden:
	P <-> { null } -> { null };
	C <-> { null, x } -> { undecided };
	Ep <-> { null } -> { null };
	Ec <-> { null } -> { null };
	A1 <-> { null } -> { null };
	A2 <-> { null } -> { null };

constraints:
	no_overlap(A1);
	fill(A2);
	All(c) <- B(C), solution(c) == x <=> Exists(a1) <- B(A1), c <- a1;
	All(a1) <- B(A1), |a1| == 2;
	All(a2) <- B(A2), |{c <- a2 | solution(c) == x}| == 2;