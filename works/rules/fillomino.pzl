structs:
	A = combine ( C , { H, V } );

domain-hidden:
	P <-> { null } -> { null };
	C <-> { 1 ... n * m } -> { 1 ... n * m, undecided };
	Ep <-> { null } -> { null };
	Ec <-> { null } -> { null };
	A <-> { 1 ... n * m } -> { 1 ... n * m, undecided };

constraints:
	fill(A);
	All(a) <- B(A), All(c) <- a, solution(a) == solution(c) && solution(a) == |a|;