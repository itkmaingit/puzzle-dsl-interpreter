structs:
	A = combine ( C , { H, V } );

domain:
	P = { null };
	C = { null, 1 ... 4};
	Ep = { null };
	Ec = { null };
	A = { null };

constraints:
	|B(A)| == 1;
	All(c) <- B(C), solution(c) <- N <=> Exists(a) <- B(A), c <- a && solution(c) == |{ co <- connect(c, {H, V}) | solution(co) <- N }| && All(co) <- connect(c, {H, V}), solution(co) != solution(c);
